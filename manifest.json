{
   "background": {
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": {
         "128": "icon128.png"
      },
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "jquery.js", "script.js" ],
      "matches": [ "*://kahoot.it/*", "*://kahoot-smash.tk/*" ],
      "run_at": "document_idle"
   } ],
   "description": "Smash any Kahoot!",
   "icons": {
      "128": "icon128.png"
   },
   "manifest_version": 2,
   "name": "Kahoot Smasher Beta",
   "permissions": [ "webRequestBlocking", "webRequest", "storage", "cookies", "*://kahoot.it/*" ],
   "short_name": "SmashIt2!",
   "version": "0.6",
   "web_accessible_resources": [ "smasherBot.js" ]
}
