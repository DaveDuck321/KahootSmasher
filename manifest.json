{
    "name": "Kahoot Smasher",
    "short_name": "SmashIt!",
    "manifest_version": 2,
    "description":"Smash any Kahoot!",
    "permissions": [
          "storage",
          "cookies",
          "*://kahoot.it/*"
        ],
    "web_accessible_resources": [
            "smasherBot.js"
        ],
    "browser_action": {
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        },
      "default_popup": "popup.html"
    },
    "background": {
      "page": "background.html"
    },
    "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
    "content_scripts": [
        {
         "matches": ["*://kahoot.it/*","*://kahoot-smash.tk/*"],
	 "js": ["jquery.js","script.js"],
         "all_frames":true,
         "run_at":"document_idle"
        }
    ],
    "version":"6.0.2"
}